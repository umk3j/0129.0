proc show_news {path} {
	global region
	global lang
	#
	set data [::base64::decode ""]
	set img [image create "photo" -data $data -format "gif"]
	set announcement_text [msgcat::mc "announcement_$region"]
	destroy ${path}.lf 
	grid [ttk::frame ${path}.lf -padding "10 10 10 10"] -column 0 -row 0 -sticky news
	grid [ttk::frame ${path}.lf.row0 ] -column 0 -row 0 -sticky news
	grid [ttk::label ${path}.lf.row0.picture -wraplength 600 -image $img ] -column 0 -row 0 -sticky w 
	grid [ttk::label ${path}.lf.row0.dummy ] -column 1 -row 0 -padx 10 
	grid [ttk::label ${path}.lf.row0.label -wraplength 600 -text $announcement_text ] -column 2 -row 0 -sticky w 
msgcat::mcset en "mame_matches" "Join the MAME Matches group"
msgcat::mcset ru "mame_matches" "Присоединится к группе MAME Matches"
grid [ttk::button ${path}.lf.b1 -text [msgcat::mc mame_matches] -command "exec {*}[auto_execok start] https://discord.gg/SJ7VW3WS49"] -column 0 -row 1 -padx 5 -pady 5 -sticky e
	grid [ttk::separator ${path}.lf.s -orient horizontal] -column 0 -row 2 -sticky we -pady 5
	grid [ttk::frame ${path}.lf.b] -column 0 -row 10 -sticky e
	grid [ttk::button ${path}.lf.b.f -text [msgcat::mc "anonymous"] -command {exec {*}[auto_execok start] [msgcat::mc "anonymous_link"]}] -column 0 -row 0 -sticky e
	grid [ttk::button ${path}.lf.b.c -text [msgcat::mc "write"] -command "show_contacts $path"] -column 1 -row 0 -sticky e
	grid [ttk::button ${path}.lf.b.d -text [msgcat::mc "donate"] -command {exec {*}[auto_execok start] "https://donate.stream/kohr"}] -column 2 -row 0 -sticky e
	foreach w [winfo children ${path}.lf.b] {grid configure $w -padx 5 -pady 5}
}

proc show_contacts {path} {
	global region
	global lang
	destroy ${path}.lf 
	#
	set fb_icon_data [::base64::decode ""]
	set tg_icon_data [::base64::decode ""]
	set dsc_icon_data [::base64::decode ""]
	set tg_image [image create "photo" -data $tg_icon_data -format "gif"]
	set fb_image [image create "photo" -data $fb_icon_data -format "gif"]
	set dsc_image [image create "photo" -data $dsc_icon_data -format "gif"]
	grid [ttk::frame ${path}.lf -padding "10 10 10 10"] -column 0 -row 0 -sticky news -columnspan 10
	#
	grid [ttk::button ${path}.lf.b -text [msgcat::mc "back"] -command "show_news $path"] -column 0 -row 0 -sticky w
	#
	grid [ttk::separator ${path}.lf.s1 -orient horizontal] -column 0 -row 5 -sticky we -columnspan 10
	#
	grid [ttk::frame ${path}.lf.r0 -padding "10 10 10 10"] -column 0 -row 7 -sticky news -columnspan 10
	grid [ttk::label ${path}.lf.r0.l2 -text "Telegram / Facebook / Discord: edetnaozero" ] -column 0 -row 0 -sticky w
	grid [ttk::button ${path}.lf.r0.b2 -text [msgcat::mc "copy"] -command {clipboard clear; clipboard append {edetnaozero}}] -column 1 -row 0 -sticky w 
	#
	grid [ttk::frame ${path}.lf.r1 -padding "10 10 10 10"] -column 0 -row 10 -sticky news -columnspan 10
	grid [ttk::button ${path}.lf.r1.b3 -text [msgcat::mc "open"] -command {exec {*}[auto_execok start] "https://t.me/edetnaozero"} -compound right -image $tg_image] -column 0 -row 0 -sticky w
	grid [ttk::button ${path}.lf.r1.b0 -text [msgcat::mc "open"] -command {exec {*}[auto_execok start] "https://fb.com/edetnaozero"} -compound right -image $fb_image] -column 1 -row 0 -sticky w
	grid [ttk::button ${path}.lf.r1.b4 -text [msgcat::mc "open"] -command {exec {*}[auto_execok start] "https://discordapp.com/users/212446905660211200"} -compound right -image $dsc_image] -column 2 -row 0 -sticky w
	#
	grid [ttk::separator ${path}.lf.s2 -orient horizontal] -column 0 -row 35 -sticky we -columnspan 10
	#
	foreach w [winfo children ${path}.lf] {
		grid configure $w -padx 5 -pady 5
	}
	foreach w [winfo children ${path}.lf.r0] {
		grid configure $w -padx 5
	}
	foreach w [winfo children ${path}.lf.r1] {
		grid configure $w -padx 5 
	}
}

